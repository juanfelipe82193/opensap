<html>
<head>
<title>apf.modeler TestSuite</title>
<script type="text/javascript" src="../../../../../../resources/sap/ui/qunit/qunit-redirect.js"></script>
</head>
<body>

	<script
			id="sap-ui-bootstrap"
			src='../../../../../../resources/sap-ui-core.js'>
	</script>
	<script>
		/**
		 * Method that is called by the Selenium/QUnit framework to retrieve the test suites.  
		 */
		function suite() {
			var suite = new top.jsUnitTestSuite();
			var contextPath = "/" + window.location.pathname.split("/")[1];
			var ie9 = (sap.ui.Device.browser.internet_explorer && sap.ui.Device.browser.version === 9);
			if (!ie9) {
				suite.addTestPage(contextPath + "/test-resources/sap/apf/modeler/integration/tApplicationHandler.qunit.html?responderOff=true");
				suite.addTestPage(contextPath + "/test-resources/sap/apf/modeler/integration/tConfigurationModeler.qunit.html?responderOff=true");
				suite.addTestPage(contextPath + "/test-resources/sap/apf/modeler/integration/tCleanUpTextPool.html?responderOff=true");
			}
			return suite;
		}
	</script>
</body>
</html>