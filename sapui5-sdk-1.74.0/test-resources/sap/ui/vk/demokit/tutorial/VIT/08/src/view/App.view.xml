<mvc:View controllerName="colladaLoader.controller.App"
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	xmlns:vk="sap.ui.vk"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true">
	<Shell>
		<App id="colladaLoader">
			<Page title="{i18n>pageTitle}">
				<vk:Viewer id="viewer">
					<vk:contentResources>
						<vk:ContentResource source="models/model.dae" sourceType="dae" name="Collada Model"/>
					</vk:contentResources>
				</vk:Viewer>
			</Page>
		</App>
	</Shell>
</mvc:View>