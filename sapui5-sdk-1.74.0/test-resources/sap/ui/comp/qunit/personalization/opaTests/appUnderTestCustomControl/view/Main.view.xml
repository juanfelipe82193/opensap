	xmlns="sap.m"
	controllerName="view.Main"
	class="sapUiSizeCompact">

	<App>
		<Page showHeader="false">
			<content>
				<VBox id="IDVBox" width="100%">
					<items>
						<Toolbar>
							<ToolbarSpacer/>
							<Button icon="sap-icon://action-settings" press="onP13nDialogPress"/>
