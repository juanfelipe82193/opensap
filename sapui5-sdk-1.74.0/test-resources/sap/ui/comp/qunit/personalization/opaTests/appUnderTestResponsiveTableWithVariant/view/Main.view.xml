<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	xmlns:smarttable="sap.ui.comp.smarttable"
	class="sapUiSizeCompact"
	controllerName="view.Main">

	<App>
		<Page showHeader="false">
			<content>
				<smarttable:SmartTable id="IDSmartTable" tableType="ResponsiveTable" editable="false" enableAutoBinding="true"
					entitySet="ProductCollection"
					useVariantManagement="true" persistencyKey="PKeyAppUnderTestResponsiveTableWithVariant"
					useTablePersonalisation="true"
					header="ProductCollection" showRowCount="true" useExportToExcel="false"/>
			</content>
		</Page>
	</App>

</mvc:View>
