<mvc:View
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:smartTable="sap.ui.comp.smarttable"
		controllerName="sap.ui.comp.sample.smartmultieditfriendly.Page"
		xmlns="sap.m">
	<smartTable:SmartTable
			id="tblUserData"
			entitySet="Employees"
			tableType="ResponsiveTable"
			useExportToExcel="false"
			useVariantManagement="false"
			showVariantManagement="false"
			useTablePersonalisation="false"
			showTablePersonalisation="false"
			header="Employees"
			enableAutoBinding="true">
		<smartTable:customToolbar>
			<OverflowToolbar design="Transparent">
				<ToolbarSpacer/>
				<Button type="Transparent" id="btnMultiEdit" text="Edit Multiple" enabled="false" press="onOpenMultiEdit"/>
			</OverflowToolbar>
		</smartTable:customToolbar>
	</smartTable:SmartTable>
</mvc:View>