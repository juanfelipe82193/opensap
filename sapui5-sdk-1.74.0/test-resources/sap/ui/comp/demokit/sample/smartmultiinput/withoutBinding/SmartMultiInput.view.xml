<core:View xmlns:core="sap.ui.core"
		   xmlns:smartMultiInput="sap.ui.comp.smartmultiinput"
		   xmlns:smartForm="sap.ui.comp.smartform"
		   controllerName="sap.ui.comp.sample.smartmultiinput.withoutBinding.SmartMultiInput" class="sapUiSizeCompact"
		   xmlns="sap.m">


	<smartForm:SmartForm editable="true" id="form" editTogglable="true">
		<smartForm:Group label="Filters">
			<smartForm:GroupElement>
				<smartMultiInput:SmartMultiInput value="{CategoryId}" entitySet="Categories" supportRanges="true"
												 textLabel="Category with Value Help from Annotations"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartMultiInput:SmartMultiInput value="{Price}" entitySet="Categories"
												 textLabel="Category Product Value Without Support Ranges"/>
			</smartForm:GroupElement>

			<smartForm:GroupElement>
				<smartMultiInput:SmartMultiInput value="{Description}" entitySet="Categories"/>
			</smartForm:GroupElement>

			<smartForm:GroupElement>
				<smartMultiInput:SmartMultiInput value="{Price}" entitySet="Categories" supportRanges="true" textLabel="Category Product Value"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartMultiInput:SmartMultiInput value="{Boolean}" entitySet="Categories" supportRanges="true"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartMultiInput:SmartMultiInput value="{DateTime}" entitySet="Categories" supportRanges="true" textLabel="Date and Time"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartMultiInput:SmartMultiInput value="{Decimal}" entitySet="Categories" supportRanges="true"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartMultiInput:SmartMultiInput value="{DateTimeOffset}" entitySet="Categories" supportRanges="true" textLabel="Date and Time Offset"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartMultiInput:SmartMultiInput value="{Guid}" entitySet="Categories" />
			</smartForm:GroupElement>
		</smartForm:Group>
	</smartForm:SmartForm>

</core:View>
