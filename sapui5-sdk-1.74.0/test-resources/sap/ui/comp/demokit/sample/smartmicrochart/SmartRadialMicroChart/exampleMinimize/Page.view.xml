<mvc:View
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.ui.comp.smartmicrochart"
	xmlns:m="sap.m"
	controllerName="sap.ui.comp.sample.smartmicrochart.SmartRadialMicroChart.exampleMinimize.Page"
	height="100%">
	<m:Page id="page" title="SmartRadialMicroChart criticality calculation minimize direction">
		<m:Panel headerText="Positive">
			<SmartRadialMicroChart id="minimizeSmartChart" entitySet="Products">
				<customData>
					<core:CustomData key="chartQualifier" value="DonutChartQualifier" />
				</customData>
			</SmartRadialMicroChart>
		</m:Panel>
		<m:Panel headerText="Critical">
			<SmartRadialMicroChart id="minimizeSmartChartWarning" entitySet="Products">
				<customData>
					<core:CustomData key="chartQualifier" value="DonutChartQualifier" />
				</customData>
			</SmartRadialMicroChart>
		</m:Panel>
		<m:Panel headerText="Negative">
			<SmartRadialMicroChart id="minimizeSmartChartError" entitySet="Products">
				<customData>
					<core:CustomData key="chartQualifier" value="DonutChartQualifier" />
				</customData>
			</SmartRadialMicroChart>
		</m:Panel>
	</m:Page>
</mvc:View>
