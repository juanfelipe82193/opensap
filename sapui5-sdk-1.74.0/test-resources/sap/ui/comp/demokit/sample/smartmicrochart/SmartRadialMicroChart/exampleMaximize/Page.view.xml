<mvc:View
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.ui.comp.smartmicrochart"
	xmlns:m="sap.m"
	controllerName="sap.ui.comp.sample.smartmicrochart.SmartRadialMicroChart.exampleMaximize.Page"
	height="100%">
	<m:Page id="page" title="SmartRadialMicroChart criticality calculation maximize direction">
		<m:Panel headerText="Positive">
			<SmartRadialMicroChart id="maximizeSmartChart" entitySet="Products">
				<customData>
					<core:CustomData key="chartQualifier" value="DonutChartQualifier" />
				</customData>
			</SmartRadialMicroChart>
		</m:Panel>
		<m:Panel headerText="Critical">
			<SmartRadialMicroChart id="maximizeSmartChartWarning" entitySet="Products">
				<customData>
					<core:CustomData key="chartQualifier" value="DonutChartQualifier" />
				</customData>
			</SmartRadialMicroChart>
		</m:Panel>
		<m:Panel headerText="Negative">
			<SmartRadialMicroChart id="maximizeSmartChartError" entitySet="Products">
				<customData>
					<core:CustomData key="chartQualifier" value="DonutChartQualifier" />
				</customData>
			</SmartRadialMicroChart>
		</m:Panel>
	</m:Page>
</mvc:View>
