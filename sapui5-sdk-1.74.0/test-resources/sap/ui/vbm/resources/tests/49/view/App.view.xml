<mvc:View class="test-view" xmlns:chart="sap.suite.ui.microchart" controllerName="vbm-regression.tests.49.controller.App" xmlns:l="sap.ui.layout" xmlns="sap.m" xmlns:vbm="sap.ui.vbm" xmlns:mvc="sap.ui.core.mvc">


	<FlexBox direction="Row" width="100%" height="100%">
		<items>

			<Panel height="100%" class="position-absolute">
				<layoutData>
					<FlexItemData baseSize="40%"></FlexItemData>
				</layoutData>
				<content>
					<l:VerticalLayout width="100%">
						<Label text="The aim: " design="Bold"></Label>
						<Text text="To test the Visual Business GeoMap Control with a Heatmap."></Text>
						<Label class="voffset-25" text="To test:" design="Bold"></Label>
						<Text text='1. Pull the sliders and observe how the heat map changes.'></Text>
						<Label text="Scale Values"></Label>
						<Slider enableTickmarks="true" min="0.1" max="5" step="0.1" value="{/heatmap/valueScale}"></Slider>
						<Label text="Scale Radius"></Label>
						<Slider enableTickmarks="true" min="0.05" max="0.5" step="0.01" value="{/heatmap/radiusScale}"></Slider>
						<Label text="Exponent Alpha Channel"></Label>
						<Slider enableTickmarks="true" min="0.1" max="2" step="0.1" value="{/heatmap/alphaExponent}"></Slider>
						<Label text="Exponent Color"></Label>
						<Slider enableTickmarks="true" min="0.1" max="3" step="0.1" value="{/heatmap/colorExponent}"></Slider>
						<Text class="voffset-10" text="Press the buttons below and check that the heatmap changes behaviour and transitions to Density map, Intermediate map and back to Heatmap"></Text>
						<l:HorizontalLayout>
							<Button text="Density map" press="onPressDensity" class="spacer-10"></Button>
							<Button text="Intermediate map" press="onPressIntermediate" class="spacer-10"></Button>
							<Button text="Heatmap" press="onPressHeatmap" class="spacer-10"></Button>
						</l:HorizontalLayout>
					</l:VerticalLayout>

				</content>
			</Panel>

			<vbm:GeoMap
					id="vbi"
					height="100%"
					width="100%"
					mapConfiguration="{/map/mapConfiguration}" class="position-absolute">
				<vbm:layoutData>
					<FlexItemData baseSize="60%"></FlexItemData>
				</vbm:layoutData>
				<vbm:resources>
					<vbm:Resource
						name="gradientbunt.png"
						value="">
					</vbm:Resource>
				</vbm:resources>
				<vbm:vos>
					<vbm:Heatmap
								 id="my-heatmap"
								 gradient="{/heatmap/gradient}"
								 opacity="{/heatmap/opacity}"
								 behavior="{/heatmap/behavior}"
								 radiusScale="{/heatmap/radiusScale}"
								 valueScale="{/heatmap/valueScale}"
								 alphaExponent="{/heatmap/alphaExponent}"
								 colorExponent="{/heatmap/colorExponent}"
								 items="{/samples}">
						<vbm:items>
							<vbm:HeatPoint position="{P}" value="{V}" radius="{V}">
							</vbm:HeatPoint>
						</vbm:items>
					</vbm:Heatmap>
				</vbm:vos>
				
			</vbm:GeoMap>

		</items>
	</FlexBox>


</mvc:View>
