{
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": "sap.ui.vbdemos.component.App",
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sap.ui.vbdemos",
				"viewName": "Main",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "show",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [{
				"pattern": "",
				"name": "appHome",
				"target": "home",
				"description": "Example Gallery"			
				
			},{
				"pattern": "selection",
				"name": "selection",
				"target": "selection",
				"description": "Selection Example",
				"image":"./images/thumbnail.screen.selection.png", 
				"published" : "true"
				
			},{
				"pattern": "drilldown",
				"name": "drilldown",
				"target": "drilldown",
				"description" : "Drilldown Example",
				"image":"./images/thumbnail.screen.drilldown.png", 
				"published" : "true"
				
			},{
				"pattern": "dragndrop",
				"name": "dragndrop",
				"target": "dragndrop",
				"description" : "Drag and Drop Example",
				"image":"./images/thumbnail.screen.dragndrop.png", 
				"published" : "true"
				
			},{
				"pattern": "analytic",
				"name": "analytic",
				"target": "analytic",
				"description" : "Analytic Example",
				"image":"./images/thumbnail.screen.analytic.png", 
				"published" : "true"
				
			},{
				"pattern": "cluster",
				"name": "cluster",
				"target": "cluster",
				"description" : "Clustering Example",
				"image":"./images/thumbnail.screen.cluster.png", 
				"published" : "true"
				
			},{
				"pattern": "travel",
				"name": "travel",
				"target": "travel",
				"description" : "Timeslider Example",
				"image":"./images/thumbnail.screen.travel.png", 
				"published" : "wip"
			},{
				"pattern": "tour",
				"name": "tour",
				"target": "tour",
				"description" : "Tour Routing Example",
				"image":"./images/thumbnail.screen.tour.png", 
				"published" : "true"
			}],
			"targets": {
				"home": {
					"viewName": "component.Home",
					"viewLevel" : 1
				},
				"selection": {
					"viewPath": "sap.ui.vbdemos.selection",
					"viewLevel" : 2
				},
				"drilldown": {
					"viewPath": "sap.ui.vbdemos.drilldown",
					"viewLevel" : 2
				},
				"dragndrop": {
					"viewPath": "sap.ui.vbdemos.dragndrop",
					"viewLevel" : 2
				},		
				"analytic": {
					"viewPath": "sap.ui.vbdemos.analytic",
					"viewLevel" : 2
				},		
				"cluster": {
					"viewPath": "sap.ui.vbdemos.cluster",
					"viewLevel" : 2
				},	
				"travel": {
					"viewPath": "sap.ui.vbdemos.travel",
					"viewLevel" : 2
				},		
				"tour": {
					"viewPath": "sap.ui.vbdemos.tour",
					"viewLevel" : 2
				},					
				"notFound": {
					"viewName": "component.NotFound",
					"transition": "show"
				}				
			}
		}
	}
}
