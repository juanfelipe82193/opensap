<mvc:View
    controllerName="sap.ui.demo.worklist.controller.Object"
    xmlns="sap.m"
    xmlns:vm="sap.ui.fl.variants"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:uxap="sap.uxap"
>
    <uxap:ObjectPageLayout id="ObjectPageLayout">
        <uxap:headerTitle>
            <uxap:ObjectPageHeader objectTitle="Title" id="ObjectPageHeaderTitle"/>
        </uxap:headerTitle>

        <uxap:headerContent>
            <vm:VariantManagement id="variantManagementPage" for="ObjectPageLayout" updateVariantInURL="true"></vm:VariantManagement>
        </uxap:headerContent>

        <uxap:sections>
            <uxap:ObjectPageSection id="ObjectPageSectionWithButtons1" title="first Section">
                <uxap:ObjectPageSubSection  id="ObjectPageSubSectionWithButtons1" title="{SubSectionTitle1}">
                    <Button text="{Attribute1}" id="buttonAttribute1" />
                    <Button text="{Attribute2}" id="buttonAttribute2" />
                </uxap:ObjectPageSubSection>
            </uxap:ObjectPageSection>
            <uxap:ObjectPageSection id="ObjectPageSectionWithButtons2" title="second Section">
                <uxap:ObjectPageSubSection  id="ObjectPageSubSectionWithButtons2" title="{SubSectionTitle1}">
                    <Button text="{UnitNumber}" id="buttonUnitNumber" />
                </uxap:ObjectPageSubSection>
            </uxap:ObjectPageSection>
        </uxap:sections>
    </uxap:ObjectPageLayout>
</mvc:View>