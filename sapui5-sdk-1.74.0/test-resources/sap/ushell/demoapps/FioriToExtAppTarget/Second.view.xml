<core:View controllerName="sap.ushell.demo.FioriToExtAppTarget.Second"
		   xmlns:core="sap.ui.core" xmlns="sap.m">

	<Page title="Application To Receive Parameters - SECOND View">
		<content>
			<Panel headerText="The received parameters:">
				<Label text="Parameter #1 Received"/>
				<TextArea rows="5" id="txtParam1" value="{/param1}" width="100%" enabled="true"></TextArea>
				<Label text="Parameter #2 Received"/>
				<TextArea rows="5" id="txtParam2" value="{/param2}" width="100%" enabled="true"></TextArea>
				<Label text="Parameter #3 Received"/>
				<Input id="txtParam3" enabled="true" value="{/param3}"/>
			</Panel>
		</content>
	</Page>

</core:View>