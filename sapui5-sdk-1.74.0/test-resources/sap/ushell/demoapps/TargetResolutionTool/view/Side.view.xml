<mvc:View
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
    controllerName="sap.ushell.demo.TargetResolutionTool.view.Side">
    <Page title="Target Resolution Tool">
        <content>
            <List headerText="Menu">
                <items>
                    <StandardListItem type="Active" icon="sap-icon://action-settings" press="onItemPressed" key="SettingsItem" title="Settings" />
                    <StandardListItem type="Active" icon="sap-icon://travel-request" press="onItemPressed" key="IntentResolutionItem" title="Intent Resolution" />
                    <StandardListItem type="Active" icon="sap-icon://list" press="onItemPressed" key="InboundsBrowserItem" title="Inbounds Browser" />
                    <StandardListItem type="Active" icon="sap-icon://multi-select" press="onItemPressed" key="GetEasyAccessSystemsItem" title="Get Easy Access Systems" />
                </items>
            </List>
        </content>
    </Page>
</mvc:View>
