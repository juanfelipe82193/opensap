{
    "sap.flp": {
        "columns": "2",
        "rows": "1"
    },
    "sap.app": {
        "type": "card"
    },
    "sap.ui5": {
        "services": {
            "IntentBasedNavigation": {
                "factoryName": "sap.ushell.ui5service.CardNavigation"
            }
        }
    },
    "sap.card": {
        "type": "List", "//TODO": "remove type property as soon as header-only card is possible",
        "header": {
            "title": "App Finder",
            "subTitle": "",
            "icon": {
                "src": "sap-icon://sys-find"
            },
            "actions": [
                {
                    "type": "Navigation",
                    "enabled": true,
                    "target": "_self",
                    "url": "#Shell-appfinder"
                }
            ]
        }
    }
}