<mvc:View
	controllerName="sap.ushell.sample.OpaFLPSandbox.controller.Master"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	xmlns:semantic="sap.m.semantic">

	<semantic:MasterPage
		id="page"
		title="Master">

		<semantic:content>
			<List
				id="list"
				items="{path: '/Objects'}"
				noDataText="No objects found">
				<items>
					<ObjectListItem
						type="Active"
						press="onSelectObject"
						title="{Name}"/>
				</items>
			</List>
		</semantic:content>
	</semantic:MasterPage>
</mvc:View>
