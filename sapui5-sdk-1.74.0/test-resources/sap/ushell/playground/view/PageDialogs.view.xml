<View
        xmlns="sap.m"
        controllerName="sap.ushell.playground.controller.PageDialogs">
    <Page title="Page Composer Dialogs" class="sapUiStdPage sapUiContentPadding">
        <VBox>
            <Button text="Open CreatePage Dialog" press=".openCreatePageDialog" />
            <Button text="Open CreatePage Dialog with ABAP transport" press=".openCreatePageDialogWithTransport" />
            <Button text="Open DeletePage Dialog" press=".openDeletePageDialog" />
            <Button text="Open DeletePage Dialog with ABAP transport" press=".openDeletePageDialogWithTransport" />
            <Button text="Open EditPage Dialog with ABAP transport" press=".openEditPageDialogWithTransport" />
            <Button text="Open CopyPage Dialog" press=".openCopyPageDialog" />
            <Button text="Open CopyPage Dialog with ABAP transport" press=".openCopyPageDialogWithTransport" />
        </VBox>

    </Page>
</View>