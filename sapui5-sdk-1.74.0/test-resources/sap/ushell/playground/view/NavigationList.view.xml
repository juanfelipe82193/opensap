<View xmlns="sap.m" controllerName="sap.ushell.playground.controller.NavigationList">
    <Page title="Controls">
        <List items="{pages>/}">
            <items>
                <StandardListItem title="{pages>title}" type="Navigation" press=".navigateTo(${pages>route})" />
            </items>
        </List>
    </Page>
</View>