<mvc:View controllerName="sap.suite.ui.microchart.sample.HarveyBallMicroChartInGenericTile.Page"
	xmlns="sap.suite.ui.microchart" xmlns:mvc="sap.ui.core.mvc" xmlns:m="sap.m">
	<m:GenericTile header="Share of Votes Processed"
		frameType="TwoByOne" press="press">
		<m:tileContent>
			<m:TileContent footer="Middle Red">
				<m:content>
					<HarveyBallMicroChart total="100" size="Responsive" alignContent="Right"
						totalScale="Mrd">
						<items>
							<HarveyBallMicroChartItem fraction="15"
								color="Error" fractionScale="Mrd" />
						</items>
					</HarveyBallMicroChart>
				</m:content>
			</m:TileContent>
			<m:TileContent footer="Small Red">
				<m:content>
					<HarveyBallMicroChart total="100" size="Responsive" alignContent="Right"
						totalScale="Mrd">
						<items>
							<HarveyBallMicroChartItem fraction="15"
								color="Error" fractionScale="Mrd" />
						</items>
					</HarveyBallMicroChart>
				</m:content>
			</m:TileContent>
		</m:tileContent>
	</m:GenericTile>
	<m:GenericTile header="Share of Votes Processed"
		frameType="OneByOne" press="press">
		<m:tileContent>
			<m:TileContent footer="Middle Green">
				<m:content>
					<HarveyBallMicroChart total="100" size="Responsive" alignContent="Right"
						totalScale="Mrd">
						<items>
							<HarveyBallMicroChartItem fraction="63.5"
								color="Good" fractionScale="Mrd" />
						</items>
					</HarveyBallMicroChart>
				</m:content>
			</m:TileContent>
		</m:tileContent>
	</m:GenericTile>
</mvc:View>
