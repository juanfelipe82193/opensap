<mvc:View
		controllerName="sap.suite.ui.commons.sample.NetworkGraphNodeDimensions.NetworkGraph"
		xmlns="sap.suite.ui.commons.networkgraph"
		xmlns:layout="sap.suite.ui.commons.networkgraph.layout"
		xmlns:mvc="sap.ui.core.mvc"
		height="100%">
	<Graph  enableWheelZoom="false"
			nodes="{/nodes}"
			id="graph"
			orientation="LeftRight"
			height="100%"
			width="100%">
		<layoutAlgorithm>
			<layout:NoopLayout/>
		</layoutAlgorithm>
		<nodes>
			<Node x="{x}"
				  y="{y}"
				  key="{key}"
				  title="{title}"
				  titleLineSize="{titleLineSize}"
				  descriptionLineSize="{descriptionLineSize}"
				  icon="{icon}"
				  attributes="{path:'attributes', templateShareable:true}"
				  width="{width}"
				  maxWidth="{maxWidth}"
				  description="{description}"
				  height="{height}"
				  shape="{shape}">
				<attributes>
					<ElementAttribute
							label="{label}"
							value="{value}"/>
				</attributes>
			</Node>
		</nodes>
	</Graph>
</mvc:View>
