<mvc:View
		controllerName="sap.suite.ui.commons.sample.NetworkGraphTwoColumns.NetworkGraph"
		xmlns="sap.suite.ui.commons.networkgraph"
		xmlns:mvc="sap.ui.core.mvc"
		height="100%">
	<Graph enableWheelZoom="false"
		   id="graph"
		   height="100%"
		   width="100%"
		   enableZoom="false"
		   nodes="{/nodes}"
		   lines="{/lines}"
		   groups="{/groups}">
		<nodes>
			<Node
					key="{key}"
					title="{title}"
					group="{group}"
					icon="{icon}"
					attributes="{path:'attributes', templateShareable:true}"
					showActionLinksButton="false"
					status="{status}"
					shape="Box">
				<attributes>
					<ElementAttribute
							label="{label}"
							value="{value}"/>
				</attributes>
			</Node>
		</nodes>
		<lines>
			<Line
					from="{from}"
					to="{to}"
					attributes="{path:'attributes', templateShareable:true}"
					status="{status}">
				<attributes>
					<ElementAttribute
							label="{label}"
							value="{value}"/>
				</attributes>
			</Line>
		</lines>
		<groups>
			<Group
					collapsed="{collapsed}"
					icon="{icon}"
					parentGroupKey="{parentGroupKey}"
					key="{key}"
					title="{title}">
			</Group>
		</groups>
	</Graph>
</mvc:View>
