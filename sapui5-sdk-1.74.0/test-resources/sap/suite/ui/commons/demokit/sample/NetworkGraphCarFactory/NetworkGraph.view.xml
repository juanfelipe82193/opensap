<mvc:View
		controllerName="sap.suite.ui.commons.sample.NetworkGraphCarFactory.NetworkGraph"
		xmlns="sap.suite.ui.commons.networkgraph"
		xmlns:mvc="sap.ui.core.mvc"
		height="100%">
	<Graph id="graph"
		   enableWheelZoom="false">
		<nodes>
			<Node key="Node1" title="Node1" status="Error" height="30"/>
			<Node key="Node2" title="Node2"/>
			<Node key="Node3" title="Node3" shape="Box" width="50"/>
		</nodes>
		<lines>
			<Line from="Node1" to="Node2" status="Error" />
			<Line from="Node3" to="Node1" arrowPosition="Middle"/>
		</lines>
	</Graph>
</mvc:View>
