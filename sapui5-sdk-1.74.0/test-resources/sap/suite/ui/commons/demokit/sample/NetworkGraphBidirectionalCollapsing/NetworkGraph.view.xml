<mvc:View
		controllerName="sap.suite.ui.commons.sample.NetworkGraphBidirectionalCollapsing.NetworkGraph"
		xmlns="sap.suite.ui.commons.networkgraph"
		xmlns:mvc="sap.ui.core.mvc"
		height="100%">
	<Graph
			enableWheelZoom="false"
			id="graph"
			nodes="{/nodes}"
			lines="{/lines}">
		<nodes>
			<Node
					key="{key}"
					title="{title}"
					icon="{icon}"
					attributes="{path:'attributes', templateShareable:true}"
					showActionLinksButton="false"
					showDetailButton="false"
					showExpandButton="false"
					shape="Box">
				<attributes>
					<ElementAttribute
							label="{label}"
							value="{value}"/>
				</attributes>
				<actionButtons>
					<ActionButton
							icon="sap-icon://navigation-left-arrow"
							title="Expand"
							position="Left"
							press="leftExpandPressed"/>
					<ActionButton
							icon="sap-icon://close-command-field"
							title="Expand Multi"
							position="Left"
							press="leftMultiExpandPressed"/>
					<ActionButton
							icon="sap-icon://navigation-right-arrow"
							title="Expand"
							press="rightExpandPressed"/>
					<ActionButton
							icon="sap-icon://open-command-field"
							title="Expand Multi"
							press="rightMultiExpandPressed"/>
				</actionButtons>
			</Node>
		</nodes>
		<lines>
			<Line
					from="{from}"
					to="{to}">
			</Line>
		</lines>
	</Graph>
</mvc:View>
