<View xmlns="sap.m"
    xmlns:core="sap.ui.core"
    controllerName="sap.ushell.components.shell.MenuBar.controller.MenuBar">
    <IconTabHeader showOverflowSelectList="true" items="{menu>/}" selectedKey="{viewConfiguration>/selectedKey}" select=".onMenuItemSelection">
        <items>
            <IconTabFilter text="{menu>title}" key="{menu>uid}" enabled="{viewConfiguration>/enableMenuBarNavigation}"/>
        </items>
    </IconTabHeader>
</View>
