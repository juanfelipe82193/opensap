{
  "_version": "1.9.0",
  "sap.app": {
    "id": "sap.makit",
    "type": "library",
    "embeds": [],
    "applicationVersion": {
      "version": "1.74.0"
    },
    "title": "Mobile Chart controls based on the Sybase MAKIT charting lib.",
    "description": "Mobile Chart controls based on the Sybase MAKIT charting lib.",
    "resources": "resources.json",
    "offline": true
  },
  "sap.ui": {
    "technology": "UI5",
    "supportedThemes": [
      "base",
      "sap_bluecrystal"
    ]
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.74",
      "libs": {
        "sap.ui.core": {
          "minVersion": "1.74.0"
        }
      }
    },
    "library": {
      "i18n": "messagebundle.properties",
      "content": {
        "controls": [
          "sap.makit.Chart",
          "sap.makit.CombinationChart"
        ],
        "elements": [
          "sap.makit.Axis",
          "sap.makit.Category",
          "sap.makit.CategoryAxis",
          "sap.makit.Column",
          "sap.makit.Layer",
          "sap.makit.MakitLib",
          "sap.makit.Row",
          "sap.makit.Series",
          "sap.makit.Value",
          "sap.makit.ValueAxis",
          "sap.makit.ValueBubble"
        ],
        "types": [
          "sap.makit.ChartType",
          "sap.makit.LegendPosition",
          "sap.makit.SortOrder",
          "sap.makit.ValueBubblePosition",
          "sap.makit.ValueBubbleStyle"
        ],
        "interfaces": []
      }
    }
  }
}